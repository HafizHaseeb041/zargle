                                            <script>
                                                function validateForm() {
                                                    // Get the required fields
                                                    var fieldOfInterest = document.getElementById("fieldOfInterest");
                                                    var gender = document.getElementById("gender");
                                                    var ageGroup = document.getElementById("ageGroup");
                                                    var purpose = document.querySelector('input[name="purpose"]:checked');

                                                    // Check if any required field is empty or not selected
                                                    if (fieldOfInterest.value === "" || gender.value === "" || ageGroup.value === "" || !purpose) {
                                                        alert("Please fill in all the required fields.");
                                                        return false;
                                                    }

                                                    // If all fields are filled, redirect based on the purpose
                                                    if (purpose.value === "For Fun") {
                                                        window.location.href = "http://localhost:5000/";  // Redirect to localhost
                                                    } else if (purpose.value === "For Professional Work") {
                                                        window.location.href = "/chat";  // Redirect to chat page
                                                    }
                                                }
                                            </script>

